import pyautogui
import time

time.sleep(3)


#Mensagem 1
pyautogui.moveTo(255,243)
pyautogui.click()
time.sleep(2)
pyautogui.typewrite("Empreendedores contabeis",interval=0.1)
pyautogui.press("enter")
pyautogui.moveTo(631,700)
pyautogui.click()
pyautogui.typewrite("Acabei de criar uma automacao , para whatsapp , vou deixar o video aqui no grupo",interval=0.1)
pyautogui.press('enter')

#Mover o mouse para enviar o video ou imagem para mensagem 1
time.sleep(2)
pyautogui.moveTo(482,699)
pyautogui.click()
time.sleep(5)
pyautogui.moveTo(481,629)
pyautogui.click()
time.sleep(5)
pyautogui.moveTo(77,359)
pyautogui.click()
time.sleep(5)
pyautogui.moveTo(351,186)
pyautogui.doubleClick()
time.sleep(7)
pyautogui.press('enter')
time.sleep(2)

